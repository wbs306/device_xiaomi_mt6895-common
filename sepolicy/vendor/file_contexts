# Charger
/vendor/bin/batterysecret               u:object_r:batterysecret_exec:s0

# Display
/mnt/vendor/persist/display(/.*)?       u:object_r:vendor_persist_display_file:s0
/data/vendor/display(/.*)?              u:object_r:vendor_display_vendor_data_file:s0

# GPU governor
/vendor/bin/ondemand_mtk                u:object_r:gpu_governor_exec:s0
/vendor/bin/ondemand_gpu.sh             u:object_r:gpu_governor_exec:s0
/data/gpu_freq_table.conf               u:object_r:gpu_governor_data_file:s0

# Graphics
/sys/devices/virtual/mi_display/disp_feature/disp-DSI-0/brightness_clone  u:object_r:vendor_sysfs_graphics:s0

# Hals
/vendor/bin/hw/vendor\.xiaomi\.hardware\.citsensorservice@1\.1-service u:object_r:hal_citsensorservice_default_exec:s0
/(vendor|system/vendor)/bin/hw/android\.hardware\.lights-service\.xiaomi_mt6895   u:object_r:hal_light_default_exec:s0
/(vendor|system/vendor)/bin/hw/vendor\.xiaomi\.hardware\.vibratorfeature\.service    u:object_r:hal_vibrator_default_exec:s0
/vendor/bin/hw/mfp-daemon u:object_r:hal_fingerprint_default_exec:s0

# NFC
/dev/nq-nci  												u:object_r:nfc_device:s0
/dev/p73     												u:object_r:nfc_device:s0

# Power
/dev/xiaomi-touch             u:object_r:vendor_touchfeature_device:s0

# RPMB device
/dev/0:0:0:49476  u:object_r:teei_rpmb_device:s0

# Set permissive
/vendor/bin/set_permissive.sh                     u:object_r:set_permissive_exec:s0

# Thermals
/vendor/bin/mi_thermald       										u:object_r:mi_thermald_exec:s0
